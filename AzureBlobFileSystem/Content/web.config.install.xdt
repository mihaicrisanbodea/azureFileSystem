<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <appSettings xdt:Transform="Insert">
	 <add key="StorageAccountConnectionString" value="DefaultEndpointsProtocol=https;AccountName=^Storage Account^;AccountKey=^Storage Key^;BlobEndpoint=https://^Storage Account^.blob.core.windows.net/;TableEndpoint=https://^Storage Account^.table.core.windows.net/;QueueEndpoint=https://^Storage Account^.queue.core.windows.net/;FileEndpoint=https://^Storage Account^.file.core.windows.net/" />
     <add key="ContainerName" value="container-name" />
     <add key="DefaultFileName" value="temp.tmp"/>
     <add key="UtcTimeFormat" value="yyyy'-'MM'-'dd'T'HH':'mm':'ss'.'fff'Z'" />
     <add key="BlobListingPageSize" value="500" />
    </appSettings>
</configuration>
